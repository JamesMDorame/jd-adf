{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "JMD-DATAFACTORY"
		},
		"ABLB_JMDblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABLB_JMDblob'"
		},
		"ADLS_JMDadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_JMDadls'"
		},
		"ASQL_JMD_Entertaintment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_JMD_Entertaintment'"
		},
		"SQL_4KFun_Entertainment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_4KFun_Entertainment'"
		},
		"ADLS_JMDadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jmddfadls.dfs.core.windows.net"
		},
		"HTTP_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "JMD-DATAFACTORY_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABLB_JMDblob_connectionString": {
						"value": "[parameters('ABLB_JMDblob_connectionString')]"
					},
					"ADLS_JMDadls_accountKey": {
						"value": "[parameters('ADLS_JMDadls_accountKey')]"
					},
					"ASQL_JMD_Entertaintment_connectionString": {
						"value": "[parameters('ASQL_JMD_Entertaintment_connectionString')]"
					},
					"SQL_4KFun_Entertainment_connectionString": {
						"value": "[parameters('SQL_4KFun_Entertainment_connectionString')]"
					},
					"ADLS_JMDadls_properties_typeProperties_url": {
						"value": "[parameters('ADLS_JMDadls_properties_typeProperties_url')]"
					},
					"HTTP_Lego_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Lego_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}